import java.io.IOException;
import java.util.*;
public class Main {
 
    public static void main(String[] args) throws IOException {
    Scanner ler = new Scanner(System.in);
    double raio, n, area;
        raio=ler.nextDouble();
        n=3.14159;
        area =n*(Math.pow(raio, 2));
        System.out.printf("A=%.4f\n", area);
        ler.close();
    }
 
}
